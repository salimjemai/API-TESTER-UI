<Page x:Class="API_TESTER_UI.Pages.CompanyDatabase.GetAircrafts"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:API_TESTER_UI.Pages.CompanyDatabase"
      mc:Ignorable="d"
      d:DesignHeight="450"
      d:DesignWidth="800"
      Title="GetAircrafts">
  <Grid>
    <DataGrid x:Name="AirportsGrid"
              AlternatingRowBackground="Aquamarine" 
              CanUserAddRows="False"
              AutoGenerateColumns="False">
      <DataGrid.Columns>
        <DataGridTextColumn Header="Airport Name"
                            Binding="{Binding Path=_Airport}" />
      </DataGrid.Columns>
    </DataGrid>
  </Grid>
</Page>
