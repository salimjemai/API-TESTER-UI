<Page x:Class="API_TESTER_UI.Pages.CompanyDatabase.GetAircrafts"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:API_TESTER_UI.Pages.CompanyDatabase"
      mc:Ignorable="d"
      d:DesignHeight="450"
      d:DesignWidth="800"
      x:Name="window"
      Title="GetAircrafts">

  <Grid>
    <ListView Grid.Row="1"
              Grid.ColumnSpan="5"
              x:Name="AirportsGrid"
              Panel.ZIndex="1">
      <ListView.View>
        <GridView>
          <GridViewColumn Header="Airport" Width="300" DisplayMemberBinding="{Binding Airport}" />
        </GridView>
      </ListView.View>
    </ListView>
    <Button Content="Clear"
            FontWeight="Bold"
            Foreground="DarkBlue"
            Height="20"
            Width="59"
            Panel.ZIndex="2"
            Margin="0 0 1800 8500"
            Click="ClosAirportsForm_Click" />
  </Grid>
</Page>
